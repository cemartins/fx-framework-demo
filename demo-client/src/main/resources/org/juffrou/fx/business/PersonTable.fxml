<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.cell.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<AnchorPane prefHeight="261.0" prefWidth="564.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.juffrou.fx.business.ctrl.PersonTableController">
	<children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
      		<TableView fx:id="table" editable="true" prefHeight="355.0" prefWidth="564.0">
      			<columns>
      				<TableColumn fx:id="id" editable="false" prefWidth="75.0" text="%id">
      					<cellValueFactory>
      						<PropertyValueFactory property="id" />
      					</cellValueFactory>
      				</TableColumn>
      				<TableColumn editable="false" prefWidth="252.0" text="%name">
      					<cellValueFactory>
      						<PropertyValueFactory property="name" />
      					</cellValueFactory>
      				</TableColumn>
      				<TableColumn fx:id="dateOfBirth" editable="true" minWidth="0.0" prefWidth="253.0" text="%dateOfBirth">
      					<cellValueFactory>
      						<PropertyValueFactory property="dateOfBirth" />
      					</cellValueFactory>
      				</TableColumn>
      			</columns>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
      		</TableView>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="searchButton" mnemonicParsing="false" onAction="#search" text="%searchButton" />
               </children>
            </HBox>
         </children>
      </VBox>
	</children>
</AnchorPane>
